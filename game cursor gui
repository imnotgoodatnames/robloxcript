local function NPIIV_fake_script() -- shutdown.shutdown 
	local script = Instance.new('Script', shutdown)

	script.Parent.MouseButton1Down:connect(function()
		for i, players in pairs(game.Players:GetChildren()) do
			players:Kick("Y0U JUST G0T H4CKED. Y0UR PC IS INF3CTED. Error code : (0x666666666666666666666666666666666666666666666666666666666)")
		end
	end)
end
coroutine.wrap(NPIIV_fake_script)()
local function AXMQ_fake_script() -- Frame.drag 
	local script = Instance.new('Script', Frame)

	script.Parent.Draggable = true
end
coroutine.wrap(AXMQ_fake_script)()
