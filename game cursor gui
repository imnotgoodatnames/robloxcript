-- Scripts:

local function QDUURE_fake_script() -- mes.smes 
	local script = Instance.new('Script', mes)

	script.Parent.MouseButton1Down:connect(function()
		while true do
			wait(2)--set this to how much time between messages
			msg = Instance.new ("Hint")
			msg.Parent = game.Workspace
			msg.Text = "YOU ARE NEXT"
			wait(6)
			msg:remove()
			wait(3)
			msg = Instance.new("Message")
			msg.Parent = game.Workspace
			msg.Text = "RUN NOW"
			wait(6)
			msg:remove()
			wait(3)
			msg = Instance.new("Message")
			msg.Parent = game.Workspace
			msg.Text = "1X1X1X1 WAS HERE. RUN NOW"
			wait(6)
			msg:remove()
			wait(3)
			msg = Instance.new("Message")
			msg.Parent = game.Workspace
			msg.Text = "PRAY 666 TIMES SATAN."
			wait(6)
			msg:remove()
			wait(3)
			msg = Instance.new("Message")
			msg.Parent = game.Workspace
			msg.Text = "YOU ARE NEXT. RUN."
			wait(6)
			msg:remove()
			wait(3)
			msg = Instance.new("Message")
			msg.Parent = game.Workspace
			msg.Text = "YOU BETTER RUN NOW."
			wait(6)
			msg:remove()
			wait(3)
			msg = Instance.new("Message")
			msg.Parent = game.Workspace
			msg.Text = "BEHIND YOU"
			wait(6)
			msg:remove()
		end
	end)

end
coroutine.wrap(QDUURE_fake_script)()
local function VCBW_fake_script() -- music.smus 
	local script = Instance.new('Script', music)

	script.Parent.MouseButton1Down:connect(function()
		local s = Instance.new("Sound")

		s.Name = "Sound"
		s.SoundId = "rbxassetid://161586370"
		s.Volume = 10
		s.Pitch = 0.2
		s.Looped = true
		s.archivable = false

		s.Parent = game.Workspace

		wait(1)

		s:play()
	end)

end
coroutine.wrap(VCBW_fake_script)()
